{
  "schemaVersion": 1,
  "mixin": [
    "now-playing.mixins.common.json",
    "now-playing.mixins.quilt.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "now-playing",
    "version": "${version}",
    "name": "Now Playing",
    "description": "Shows a 'now playing' popup whenever a song starts to play.",
    "authors": [
      "AppleTheGolden"
    ],
    "contact": {
      "sources": "https://github.com/Scotsguy/now-playing"
    },
    "license": "MIT",
    "icon": "assets/now_playing/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "client",
    "entrypoints": {
      "client": [
        "com.github.scotsguy.nowplaying.quilt.NowPlayingQuilt"
      ],
      "modmenu": [
        "com.github.scotsguy.nowplaying.quilt.ModMenuIntegration"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.x"
      },
      {
        "id": "cloth-config2",
        "version": ">=4.6.0"
      }
    ]
  }
}
